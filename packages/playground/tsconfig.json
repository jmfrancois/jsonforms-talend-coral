{
  "extends": "@talend/scripts-config-typescript/tsconfig.json",
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "outDir": "./lib",
    "jsx": "react-jsx",
    "allowJs": true,
    "declaration": true,
    "target": "ES5",
    "module": "esnext",
    "noFallthroughCasesInSwitch": true,
    "noEmit": true
  }
}
